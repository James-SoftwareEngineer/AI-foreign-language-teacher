{
  "compilerOptions": {
  "target": "es5",
  "lib": ["dom", "esnext"],
  "jsx": "react-jsx",
  "allowJs": true,
  "skipLibCheck": true,
  "esModuleInterop": true,
  "strict": true,
  "forceConsistentCasingInFileNames": true,
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "isolatedModules": true
  },
  "include": ["src"]
  }